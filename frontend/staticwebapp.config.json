{
  "routes": [
    {
      "route": "/api/*",
      "methods": ["GET", "POST", "PUT", "DELETE"],
      "rewrite": "/api/$1"
    },
    {
      "route": "/*",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
  },
  "responseOverrides": {
    "400": {
      "rewrite": "/index.html",
      "statusCode": 200
    },
    "401": {
      "rewrite": "/index.html",
      "statusCode": 200
    },
    "403": {
      "rewrite": "/index.html",
      "statusCode": 200
    },
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}